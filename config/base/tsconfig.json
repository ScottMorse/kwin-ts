{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "experimentalDecorators": false,
    "importHelpers": true,
    "module": "ESNext",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "typeRoots": ["../../node_modules/@types", "../../types"],
    "noEmit": true,
    "moduleDetection": "force",
    "noImplicitAny": true,
    "target": "ES5",
    "baseUrl": ".",
    "types": ["bun"]
  },
  "include": ["./_file.ts"],
  "exclude": ["**/node_modules/**/*", "**/dist/**/*", "**/kwin-ts-output/**/*"]
}
