{
  "root": true,
  "ignorePatterns": ["!**/*", "dist", "output", "kwin-ts-output"],
  "plugins": ["@nx"],
  "rules": {
    "no-console": "warn"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "extends": ["plugin:@nx/typescript"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "extends": ["plugin:@nx/javascript"],
      "rules": {}
    }
  ]
}
